@model LingvaApp.Models.PublishedArticle
@{Layout = null;} 

<html>
<head>
    <title>@Model.Title</title>
</head>
<body>
    @Model.AuthorID - @Model.ArticleID
    <h1>@Model.Title</h1>
    <img src="~/assets/library.jpg" style="width:100%;height:100px;margin-bottom:40px;"/>
    @Html.Raw(Model.Content)
    @Model.Language
    @Model.Level
    @Model.CreationDate
    @Model.Rating
    @Model.Tags
</body>
</html>